{
  "name": "swoft/config",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "micro-service framework base on swoole",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1",
    "vlucas/phpdotenv": "^2.5",
    "symfony/yaml": "^4.2"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "Swoft\\Config\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Config\\Unit\\": "test/unit/",
      "SwoftTest\\Config\\Testing\\": "test/testing/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml",
    "cs-fix": "./vendor/bin/php-cs-fixer fix $1"
  }
}
