{
  "name": "swoft/log",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "micro-service framework base on swoole",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "monolog/monolog": "^2.0"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "Swoft\\Log\\": "src/"
    }
  },
  "require-dev": {
    "swoft/swoole-ide-helper": "dev-master",
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Log\\Unit\\": "test/unit/",
      "SwoftTest\\Log\\Testing\\": "test/testing/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml",
    "cs-fix": "./vendor/bin/php-cs-fixer fix $1"
  }
}
